{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production pm2 start ./build/src/app.js -i 5 --attach --watch | ./node_modules/.bin/bunyan",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts | ./node_modules/.bin/bunyan",
    "lint:check": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'",
    "build": "ttsc -p .",
    "test": "jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false",
    "seeds:dev": "npx ts-node src/seeds.ts",
    "seeds:prod": "npx ts-node ./build/src/seeds.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@socket.io/redis-adapter": "^8.2.1",
    "bunyan": "^1.8.15",
    "cloudinary": "^2.5.1",
    "compression": "^1.7.4",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "nodemon": "^3.1.7",
    "npm": "^10.9.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4",
    "socket.io": "^4.7.1",
    "ttypescript": "^1.5.15",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.4.6"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.11",
    "@types/compression": "^1.7.5",
    "@types/cookie-session": "^2.0.49",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.6",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.3.3",
    "redis": "^4.6.7",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0"
  }
}
